<StackPanel xmlns:ux="http://schemas.outracks.com/ux" xmlns:ua="Uno.Animations" ux:ClassName="LoadingElement" Orientation="Horizontal" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Visibility="Visible" Margin="0, 0, 0, 0" Padding="0, 0, 0, 0" xmlns="Uno.Scenes, Uno.UI, Uno.Drawing, Uno">
  <Image X="385" Y="195" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="4, 0, 5, 0" ux:Name="Image1">
    <texture2D ux:Binding="Texture" ux:Name="texture2D1" ux:Path="Assets/Circle.png" />
  </Image>
  <Image X="385" Y="195" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0, 0, 5, 0" Grid.Row="0" Grid.Column="0" ux:Name="Image11">
