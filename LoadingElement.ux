      <ua:FloatKeyframe LeftValue="0" Value="0" Easing="Linear" Time="1.5" />
    </ua:FloatTrack>
    <ua:FloatTrack Target="Image111.Opacity" ux:Name="FloatTrack2">
      <ua:FloatKeyframe LeftValue="0" Value="0" Easing="Linear" Time="1" ux:Name="FloatKeyframe2" />
      <ua:FloatKeyframe LeftValue="1" Value="1" Easing="Linear" Time="1.5" />
